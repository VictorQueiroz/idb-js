{
  "devDependencies": {
    "@types/chai": "4.x",
    "@types/express": "^4.17.17",
    "@types/node": "18.x",
    "autobind-decorator": "^2.4.0",
    "chai": "4.x",
    "eslint": "^8.38.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "5.x",
    "package-utilities": "0.x",
    "prettier": "^2.8.7",
    "puppeteer": "19.x",
    "ts-node": "^10.9.1",
    "typescript": "5.x",
    "utility-types": "3.x",
    "webpack": "5.x",
    "webpack-cli": "5.x"
  },
  "main": "./src/index.js",
  "scripts": {
    "build:es": "npx tsc --project src --module ESNext --outDir es",
    "build": "npx tsc -b src --force",
